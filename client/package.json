{
  "name": "@dr-kobros/hardcore-react-training",
  "version": "4.0.0",
  "description": "A training / starter project utilizing Broilerplate & React",
  "main": "src",
  "scripts": {
    "build:client": "cross-env NODE_ENV=production webpack --progress --display-optimization-bailout",
    "build:server": "cross-env NODE_ENV=production webpack --progress --config webpack.server.config.js --display-optimization-bailout",
    "build:node": "cross-env NODE_ENV=production babel src/index.js > dist-server/index.js",
    "build": "yarn run build:client && yarn run build:server && yarn run build:node",
    "lint": "eslint src",
    "babel-node": "babel-node",
    "start": "cross-env NODE_ENV=development webpack-serve --config webpack.config.js --port 8888",
    "flow": "flow",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pekkis/react-training-broilerplate"
  },
  "keywords": [
    "React",
    "example",
    "starter",
    "boilerplate",
    "broilerplate",
    "training"
  ],
  "author": "Pekkis",
  "license": "BSD-3-Clause",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(pcss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "faker": "^4.1.0",
    "final-form": "^4.10.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "luxon": "^1.4.4",
    "polished": "^2.2.0",
    "prop-types": "^15.6.2",
    "random-js": "^1.0.8",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-error-boundary": "^1.2.3",
    "react-fa": "^5.0.0",
    "react-final-form": "^3.6.7",
    "react-helmet": "^5.2.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.0.2",
    "transit-immutable-js": "^0.7.0",
    "transit-js": "^0.8.861",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@dr-kobros/broilerplate": "^0.18.0",
    "@dr-kobros/broilerplate-mini-css-extract": "^0.18.0",
    "@dr-kobros/broilerplate-postcss": "^0.18.0",
    "@dr-kobros/broilerplate-styled-components": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.x",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.8.0",
    "connect-history-api-fallback": "^1.5.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.1.0",
    "empty": "^0.10.1",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react-app": "^3.0.4",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.83.0",
    "flow-typed": "^2.5.1",
    "identity-obj-proxy": "^3.0.0",
    "import-inspector": "^2.0.0",
    "jest": "^23.6.0",
    "koa-connect": "^2.0.1",
    "normalize.css": "^8.0.0",
    "postcss-advanced-variables": "^2.3.3",
    "postcss-import": "^12.0.0",
    "postcss-modules": "^1.4.1",
    "postcss-nested": "^4.1.0",
    "postcss-nesting": "^7.0.0",
    "prettier": "^1.14.3",
    "react-inspector": "^2.3.0",
    "redux-devtools-extension": "^2.13.5",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-serve": "^2.0.2"
  }
}
